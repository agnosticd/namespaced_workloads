---
apiVersion: workspace.devfile.io/v1alpha2
kind: DevWorkspace
metadata:
  name: {{ ns_private_lmaas_namespace_devspace_name }}
  namespace: {{ _ns_private_lmaas_namespace_devspaces }}
  labels:
    guid: "{{ guid }}"
    creator: agnosticd.namespaced_workloads.ns_private_lmaas
spec:
  routingClass: che
  started: {{ ns_private_lmaas_devworkspace_start }}
  contributions:
  - name: ide
    uri: {{ ns_private_lmaas_devfile_url }}
  template:
    projects:
    - name: {{ ns_private_lmaas_devworkspace_name }}
      git:
        remotes:
          origin: {{ ns_private_lmaas_devworkspace_projects_repo_url }}
        checkoutFrom:
          revision: {{ ns_private_lmaas_devworkspace_projects_revision }}
    components:
      {{ ns_private_lmaas_devworkspace_components }}
